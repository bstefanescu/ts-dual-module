{
  "name": "ts-dual-module",
  "version": "0.6.3",
  "description": "Easily build dual module packages (esm + cjs) using typescript",
  "type": "module",
  "bin": {
    "tsmod": "bin/tsmod.mjs"
  },
  "files": [
    "lib",
    "bin",
    "templates"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc"
  },
  "keywords": [
    "typescript",
    "build",
    "exports",
    "tsc",
    "esm",
    "cjs",
    "dual-module"
  ],
  "homepage": "https://github.com/bstefanescu/ts-dual-module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bstefanescu/ts-dual-module.git"
  },
  "bugs": {
    "url": "https://github.com/bstefanescu/ts-dual-module/issues"
  },
  "author": "Bogdan Stefanescu",
  "license": "MIT",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "default": "./dist/esm/index.js",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "dependencies": {
    "enquirer": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16"
  }
}